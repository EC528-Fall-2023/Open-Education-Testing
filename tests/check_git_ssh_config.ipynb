    "import subprocess\n",
    "import os\n",
    "import sys\n",
    "\n",
    "def shelltest(CMD):\n",
    "    try:\n",
    "        result = subprocess.check_output(CMD, shell=True, stderr=subprocess.STDOUT)\n",
    "        return 0, result.decode('utf-8')\n",
    "    except subprocess.CalledProcessError as e:\n",
    "        return e.returncode, e.output.decode('utf-8')\n",
    "\n",
    "ERRORS = 0"
   ]
  },
